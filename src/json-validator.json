{
  "json.schemas": [
    {
      "fileMatch": ["./uofSettings.json"],
      "schema": {
        "type": "object",
        "description": "UOF Settings config",
        "properties": {
          "environmentPrefix": {
            "$id": "#/properties/dev",
            "type": "string",
            "title": "Environment prefix to be user on the environment name. Ex.: ${environmentPrefix}_NODE",
            "description": "",
            "default": "",
            "examples": ["DEV", "UAT", "PRD"]
          },
          "OCCRootPath": {
            "type": "string",
            "title": "Path of your OCC base folder."
          },
          "terminalType": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type of your defoult terminal.",
            "description": "",
            "default": "",
            "examples": ["cmd", "bash"]
          }
        }
      }
    }
  ]
}
