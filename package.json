{
	"name": "update-occ-file",
	"displayName": "Update OCC File",
	"description": "Utilizada para atualizar um arquivo espec√≠fico no OCC Livelo",
	"version": "0.2.3",
	"publisher": "boanergesbone",
	"icon": "uof-icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/boanascimento/update-occ-file"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.updateOCCFile",
		"onCommand:extension.getOCCWidget",
		"onCommand:extension.uofPrepare",
		"onCommand:extension.runGrab"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.updateOCCFile",
				"title": "Update OCC File"
			},
			{
				"command": "extension.getOCCWidget",
				"title": "Get OCC Widget"
			},
			{
				"command": "extension.uofPrepare",
				"title": "UOF Prepare"
			},
			{
				"command": "extension.runGrab",
				"title": "UOF Run Grab"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.updateOCCFile"
				}
			],
			"editor/title/context": [
				{
					"command": "extension.updateOCCFile"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chromedriver": "^2.38.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"chromedriver": "^80.0.1"
	}
}
